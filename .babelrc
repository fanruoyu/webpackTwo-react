{
	"presets":[
		[
			"es2015",
			{
				"modules":false
			}
		],
		"react",
		"stage-0"
	],
	"plugins":[
		"react-hot-loader/babel",
		["import", { "libraryName": "antd", "style": "css" }],
	    [
			"transform-runtime",
			{
				"helpers": false,
				"polyfill": false,
				"regenerator": true,
				"moduleName": "babel-runtime"
			}
		]
	]
}